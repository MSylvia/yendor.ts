{
  "extends": "tslint:recommended",
  "rules": {
    "no-bitwise": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-console": [
      false
    ],
    "no-empty": false,
    "object-literal-sort-keys": false
  }
}
